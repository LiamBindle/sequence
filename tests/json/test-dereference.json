{
    "include": {
        "put-param-on-stack": {
            "metadata": {
                "parameters": {
                    "value": {
                        "type": "int",
                        "description": "value to put on stack"
                    }
                }
            },
            "run": [
                {"op": "get", "uri": "parameters:value"}
            ]
        }
    },

    "run": [
        7,
        {"op": "put", "uri": "locals:seven"},
        5,
        {"op": "put", "uri": "locals:five"},
        3,
        {"op": "put", "uri": "locals:three"},
        2,
        {"op": "put", "uri": "locals:two"},
        {"op": "put-param-on-stack", "value": 1},
        {"op": "put-param-on-stack", "value": "${locals:two}"},
        {"op": "put-param-on-stack", "value": ["${locals:three}", 4]},
        {"op": "put-param-on-stack", "value": {"five": "${locals:five}", "six": 6}},
        {"op": "unpack", "keys": ["five", "six"]},
        {"op": "put-param-on-stack", "value": [{"seven": "${locals:seven}"}]},
        {"op": "unpack", "keys": ["seven"]}
    ],
    "tests": [
        {
            "name": "test procedure parameters and nested procedures",
            "checks": [
                {
                    "answer": [
                        1, 2, 3, 4, 5, 6, 7
                    ]
                }
            ]
        }
    ]
}
