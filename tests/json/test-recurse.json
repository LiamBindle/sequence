{
    "metadata": {
        "description": "Calculates the factorial of a number",
        "inputs": [
            {
                "name": "N",
                "type": "int",
                "description": "Number to calculate factorial of"
            },
            {
                "name": "b",
                "description": "This is a dummy"
            },
            {
                "name": "c"
            }
        ],
        "outputs": [
            {
                "name": "F",
                "type": "int",
                "description": "Factorial result"
            }
        ],
        "references": [
            {"url": "https://github.com/LiamBindle/collagen", "text": "VirtualMachine, GitHub."},
            "Second reference"
        ]
    },
    "run": [
        {"op": "size"},
        1,
        {"op": "eq"},
        {"op": "if"},
            1,
            {"op": "swap"},
        {"op": "endif"},

        {"op": "dup"},
        {"op": "if"},

            {"op": "dup"},
            {"op": "swap", "order": [0, 2, 1]},
            {"op": "*"},
            {"op": "swap"},
            1,
            {"op": "-"},
            {"op": "recurse"},
        {"op": "else"},
            {"op": "drop"},
        {"op": "endif"}
    ],
    "tests": [
        {
            "name": "std.recurse - factorial(5)",
            "setup": [
                5
            ],
            "checks": [
                {
                    "answer": [
                        120
                    ]
                }
            ]
        },
        {
            "name": "std.recurse - factorial(13)",
            "setup": [
                13
            ],
            "checks": [
                {
                    "answer": [
                        6227020800
                    ]
                }
            ]
        }
    ]
}
